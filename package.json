{
  "name": "nestjs-graphql-prisma-realworld-example-app",
  "private": true,
  "version": "0.0.0-dev",
  "description": "Example real world application built with NestJS, GraphQL and Prisma",
  "license": "MIT",
  "scripts": {
    "dev": "run-p start:dev prisma:w",
    "postinstall": "patch-package && npm run prisma:generate",
    "build": "sh Taskfile build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:dev": "node -r dotenv/config node_modules/@nestjs/cli/bin/nest.js start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/src/main",
    "tscheck": "echo tscheck... && tsc --noEmit --incremental false",
    "tscheck:w": "npm run tscheck -- --watch",
    "test": "echo todo",
    "test:w": "echo todo",
    "migrate:save": "node -r dotenv/config node_modules/@prisma/cli/build/index.js migrate save --experimental",
    "migrate:save:preview": "npm run migrate:save -- --preview",
    "migrate:up": "node -r dotenv/config node_modules/@prisma/cli/build/index.js migrate up --experimental --verbose",
    "migrate:up:preview": "npm run migrate:up -- --preview",
    "migrate:down": "node -r dotenv/config node_modules/@prisma/cli/build/index.js migrate down --experimental 1",
    "prisma:w": "node -r dotenv/config node_modules/@prisma/cli/build/index.js generate --watch",
    "prisma:generate": "node -r dotenv/config node_modules/@prisma/cli/build/index.js generate",
    "prisma:studio": "node -r dotenv/config node_modules/@prisma/cli/build/index.js studio --experimental",
    "sync-dotenv": "npx sync-dotenv"
  },
  "dependencies": {
    "@nestjs/common": "^6.11.11",
    "@nestjs/core": "^6.11.11",
    "@nestjs/graphql": "^6.6.2",
    "@nestjs/platform-express": "^6.11.11",
    "apollo-server-express": "^2.11.0",
    "class-transformer": "^0.2.3",
    "class-validator": "^0.11.1",
    "graphql": "^14.6.0",
    "graphql-fields": "^2.0.3",
    "graphql-info-transformer": "0.0.1",
    "graphql-tools": "^4.0.7",
    "reflect-metadata": "^0.1.13",
    "rxjs": "^6.5.4",
    "type-graphql": "^0.17.6"
  },
  "devDependencies": {
    "@nestjs/cli": "^6.14.2",
    "@nestjs/schematics": "^6.9.4",
    "@nestjs/testing": "^6.11.11",
    "@prisma/cli": "^2.0.0-alpha.980",
    "@prisma/client": "^2.0.0-alpha.980",
    "@types/express": "^4.17.3",
    "@types/graphql-fields": "^1.3.2",
    "@types/node": "^13.9.5",
    "@types/supertest": "^2.0.8",
    "dotenv": "^8.2.0",
    "npm-run-all": "^4.1.5",
    "patch-package": "^6.2.1",
    "prettier": "^2.0.2",
    "simplytyped": "^3.2.3",
    "supertest": "^4.0.2",
    "ts-loader": "^6.2.2",
    "ts-node": "^8.8.1",
    "tsconfig-paths": "^3.9.0",
    "typegraphql-prisma": "^0.1.7",
    "typescript": "^3.8.3",
    "watchexec-bin": "^1.0.0"
  }
}
